.container
    .help
        .help-grid-tools
            .help-grid-tools-body
                a.help-access(href="" target='_blank')
                    .help-access-content.help-rules.d-flex.align-items-center.justify-content-between
                        h3.title.help-text Assess Your Symptoms
                        img(src='../../../assets/images/sym.png', alt="")
                a.help-access(href="" target='_blank')
                    .help-access-setup.d-flex.align-items-center.justify-content-between
                        h3.title.help-text Set Up a Virtual Doctor Visit
                        img(src='../../../assets/images/setup.png', alt="")
                a.help-access(href="" target='_blank')
                    .help-find.help-rules.d-flex.align-items-center.justify-content-between
                        h3.title.help-text Find a Testing Center
                        img(src='../../../assets/images/hlp-fnd.png', alt="")
            .help-view-tools
                a.view-all(href="" aria-label="view all resources tools") View All Tools






